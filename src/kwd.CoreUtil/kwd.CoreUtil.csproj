<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.2.0-beta</Version>
    <Authors>Derek Kowald</Authors>
    <Product>KWD Core Utilities</Product>
    <Title>KWD Core Utilities</Title>
    <Description>A set of extensions to provide a better development experiance for .NET Core</Description>
    <PackageProjectUrl>https://github.com/Dkowald/kwd.CoreUtil</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Dkowald/kwd.CoreUtil.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <Tags>.NET Core;Utility</Tags>
		<PackageReleaseNotes>
      ## Case ignorant file access.
    </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="\" />
    <None Include="icon.svg" Pack="false" />
  </ItemGroup>

</Project>
